{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": []
  },
  "paths": {
    "@core/*": ["src/app/core/*"],
    "@store/*": ["src/app/core/store/*"],
    "@storeIndex/*": ["src/app/core/store/index"],
    "@shared/*": ["src/app/shared/*"],
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ]
}
