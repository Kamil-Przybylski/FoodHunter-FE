{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": ["googlemaps"]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "paths": {
    "@core/*": ["src/app/core/*"],
    "@store/*": ["src/app/core/store/*"],
    "@storeIndex/*": ["src/app/core/store/index"],
    "@shared/*": ["src/app/shared/*"],
  },
  "include": [
    "src/**/*.d.ts"
  ]
}
